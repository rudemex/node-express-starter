{
  "root": true,
  "plugins": ["prettier"],
  "extends": [
    "plugin:prettier/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "no-debugger": "error",
    "no-var": "error",
    "prettier/prettier":  ["error",{
      "endOfLine": "auto"}
    ]
  }
}
